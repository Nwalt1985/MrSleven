{
  "addons": [
    "cloudinary"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "CLOUDINARY_KEY": {
      "required": true
    },
    "CLOUDINARY_LINK": {
      "required": true
    },
    "CLOUDINARY_NAME": {
      "required": true
    },
    "CLOUDINARY_SECRET": {
      "required": true
    },
    "CLOUDINARY_URL": {
      "required": true
    },
    "MONGOLAB_URI": {
      "required": true
    },
    "LIVE_MYSQL_HOST": {
      "required": true
    },
    "LIVE_MYSQL_USER": {
      "required": true
    },
    "LIVE_MYSQL_PASS": {
      "required": true
    },
    "LIVE_MYSQL_DB": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "tawnyillustration_new",
  "scripts": {
  },
  "stack": "heroku-18"
}
